{
  "name": "tagged-event-proxy",
  "version": "0.4.1",
  "description": "Encapsulates tagged event data in a well-defined schema",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "prebuild": "npm run clean:build",
    "build": "gulp build",
    "clean": "npm run clean:build && npm run clean:coverage",
    "clean:build": "gulp clean",
    "clean:coverage": "rimraf .nyc_output coverage",
    "precoverage": "npm run clean:coverage",
    "coverage": "nyc npm run test:unit",
    "coverage:coveralls": "nyc report --reporter text-lcov | coveralls",
    "lint": "tslint src/**/*.ts test/**/*.ts",
    "test": "npm run lint && npm run coverage",
    "test:unit:ci": "mocha test/specs/**/*.js",
    "pretest:unit": "npm run clean:build",
    "test:unit": "mocha test/specs/**/*.ts --compilers ts:ts-node/register",
    "pretest:ci": "npm run build",
    "test:ci": "npm run lint && npm run test:unit:ci && npm run coverage",
    "precommit": "lint-staged",
    "prepublish": "not-in-install && npm test && npm run build || in-install",
    "prepush": "npm test"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gagle/tagged-event-proxy.git"
  },
  "engines": {
    "node": ">=6.9"
  },
  "author": "Gabriel Llamas",
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "include": [
      "src/**/*.ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "extension": [
      ".ts"
    ]
  },
  "devDependencies": {
    "@types/chai": "3.5.1",
    "@types/hoek": "4.1.3",
    "@types/mocha": "2.2.41",
    "@types/node": "7.0.14",
    "chai": "3.5.0",
    "del": "2.2.2",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-typescript": "3.1.6",
    "husky": "0.13.3",
    "in-publish": "2.0.0",
    "lint-staged": "3.4.0",
    "mocha": "3.3.0",
    "nyc": "10.2.0",
    "rimraf": "2.6.1",
    "ts-node": "3.0.2",
    "tslint": "5.1.0",
    "typescript": "2.3.1"
  },
  "dependencies": {
    "fulfiller": "0.1.3",
    "hoek": "4.1.1"
  }
}
